writeInterval   0.1000;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

purgeWrite	0; 

timePrecision   6;

runTimeModifiable no;

adjustTimeStep  no;

maxCo           1;

maxDeltaT       1;


