FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "50";
    object      ddtVoidfraction;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    outerWall
    {
        type            calculated;
        value           uniform 0;
    }
    innerWall
    {
        type            calculated;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
    defaultFaces
    {
        type            empty;
    }
}


