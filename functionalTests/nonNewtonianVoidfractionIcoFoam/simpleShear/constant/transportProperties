transportModel  KriegerDoughertyViscosity;//Newtonian;//


nu              nu [ 0 2 -1 0 0 0 0 ] 1e-3;

KriegerDoughertyViscosityCoeffs
{
    nu0             nu0 [ 0 2 -1 0 0 0 0 ] 1e-03;
    nuMax           nuMax [ 0 2 -1 0 0 0 0 ] 1e+03;
    nuMin           nuMin [ 0 2 -1 0 0 0 0 ] 1e-03;
    nuBar           nuBar [ 0 0 0 0 0 0 0 ] 2.5;
    voidfractionMax voidfractionMax [ 0 0 0 0 0 0 0 ] 0.65;
}




